{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#45F1C2",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#45F1C2",
          "template": "{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#0CA0D8",
          "template": "\uf07b {{ .Path }} ",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "#3C873A",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#FFE873",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#ec2729",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#9b51e0",
          "template": "\ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "go",
          "foreground": "#06aad5",
          "style": "plain",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#925837",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "dart",
          "style": "plain",
          "foreground": "#055b9c",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "#ce092f",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "nx",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "#359a25",
          "template": "<#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "#9c1006",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "azfunc",
          "style": "plain",
          "foreground": "#5398c2",
          "template": "\uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "aws",
          "style": "plain",
          "foreground": "#faa029",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "foreground": "#262626",
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "invert_powerline": true,
          "leading_diamond": "\ue0b2",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 <#262626>\ue0b2</> ",
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "\uf073 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#cd5e42",
          "template": "\ue3bf"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#CD4277",
          "template": "ðŸš€ "
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FFD700",
    "template": "{{if .Root}}\u26a1 {{end}}ðŸš€ "
  },
  "version": 2
}
